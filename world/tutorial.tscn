resource_name = "battle"
script/source = "extends Node2D


func _setup() -> void:
	$Actor1.data.evaded.connect(_phase_2.unbind(1), CONNECT_ONE_SHOT)


func _phase_2() -> void:
	await Battle.turn_ended
	
	var actor2: Actor = $Actor2.data
	Battle.add_actor(actor2)
	#Battle.add_actor($Actor3.data)
	
	actor2.turn_override = [
		actor2.follow_path.bind([
			actor2.position + Vector2i(0, -1),
			actor2.position + Vector2i(-1, -1),
			actor2.position + Vector2i(-2, -1),
			actor2.position + Vector2i(-3, -1),
		]),
		$Bridge2.on_interact,
		actor2.follow_path.bind([actor2.position + Vector2i(-3, 0)])
	]
	await actor2.turn_ended


func _ready() -> void:
	await get_tree().create_timer(2).timeout
	add_child(SFX.new(\"AHHH\", Vector2.ZERO, Vector2i(0, 1)))
"

